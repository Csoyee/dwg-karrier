<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.dwg_karrier.roys.MainActivity">

  <GridLayout
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:id="@+id/mainLayout"
      android:columnCount="2"
      android:rowCount="4"
      android:orientation="horizontal"
      android:layout_margin="0dp">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" app:srcCompat="@drawable/min10"
        android:id="@+id/min10"
        android:scaleType="centerCrop"
        android:layout_gravity="center"
        android:layout_marginBottom="0dp" android:layout_marginTop="0dp"
        android:layout_marginLeft="0dp" android:layout_marginRight="0dp"
        android:layout_rowSpan="2" android:layout_row="0"
        android:layout_column="0" android:layout_columnSpan="1"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" app:srcCompat="@drawable/min20"
        android:layout_gravity="center"
        android:id="@+id/min20"
        android:scaleType="centerCrop"
        android:layout_marginBottom="0dp" android:layout_marginTop="0dp"
        android:layout_marginLeft="0dp" android:layout_marginRight="0dp"
        android:layout_rowSpan="1" android:layout_row="0"
        android:layout_column="1" android:layout_columnSpan="1"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" app:srcCompat="@drawable/min30"
        android:layout_gravity="center"
        android:id="@+id/min30"
        android:scaleType="centerCrop"
        android:layout_marginBottom="0dp" android:layout_marginTop="0dp"
        android:layout_marginLeft="0dp" android:layout_marginRight="0dp"
        android:layout_rowSpan="1" android:layout_row="1"
        android:layout_column="1" android:layout_columnSpan="1"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" app:srcCompat="@drawable/min40"
        android:id="@+id/min40"
        android:scaleType="centerCrop"
        android:layout_marginBottom="0dp" android:layout_marginTop="0dp"
        android:layout_marginLeft="0dp" android:layout_marginRight="0dp"
        android:layout_gravity="center"
        android:layout_rowSpan="1" android:layout_row="2"
        android:layout_column="0" android:layout_columnSpan="1"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" app:srcCompat="@drawable/min50"
        android:id="@+id/min50"
        android:scaleType="centerCrop"
        android:layout_gravity="center"
        android:layout_marginBottom="0dp" android:layout_marginTop="0dp"
        android:layout_marginLeft="0dp" android:layout_marginRight="0dp"
        android:layout_rowSpan="1" android:layout_row="2"
        android:layout_column="1" android:layout_columnSpan="1"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" app:srcCompat="@drawable/min60"
        android:id="@+id/min60"
        android:scaleType="centerCrop"
        android:layout_gravity="center"
        android:layout_marginBottom="0dp" android:layout_marginTop="0dp"
        android:layout_marginLeft="0dp" android:layout_marginRight="0dp"
        android:layout_rowSpan="1" android:layout_row="3"
        android:layout_column="0" android:layout_columnSpan="1"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" app:srcCompat="@drawable/more"
        android:id="@+id/more"
        android:scaleType="centerCrop"
        android:layout_gravity="center"
        android:layout_marginBottom="0dp" android:layout_marginTop="0dp"
        android:layout_marginLeft="0dp" android:layout_marginRight="0dp"
        android:layout_rowSpan="1" android:layout_row="3"
        android:layout_column="1" android:layout_columnSpan="1"/>

  </GridLayout>
</RelativeLayout>
